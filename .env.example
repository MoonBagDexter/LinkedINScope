# JSearch API key from RapidAPI
VITE_JSEARCH_API_KEY=your_rapidapi_key_here

# Supabase Configuration
# Get these from: Supabase Dashboard -> Project Settings -> API
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# Kanban Lane Migration Thresholds
# Jobs migrate from 'new' to 'trending' at this click count
VITE_THRESHOLD_NEW_TO_TRENDING=5
# Jobs migrate from 'trending' to 'graduated' at this click count
VITE_THRESHOLD_TRENDING_TO_GRADUATED=20
